{
  "name": "shubox",
  "version": "1.0.0",
  "description": "Shubox client SDK",
  "repository": "https://github.com/shubox.io/shubox",
  "author": "Jayroh LLC",
  "contributors": ["Joel Oliveira <joel@shubox.io>"],
  "main": "dist/shubox.umd.js",
  "module": "index.js",
  "license": "MIT",
  "dependencies": {
    "@shubox/core": "^1.0.0"
  },
  "scripts": {
    "prepack": "cp ../../README.md .",
    "dev": "webpack --mode development",
    "build": "webpack --mode production && echo \" window.Shubox = (window.Shubox || {default: null}).default;\" >> ./dist/shubox.umd.js && cp ./dist/shubox.umd.js ../@shubox/examples/public/shubox.umd.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "webpack": "^4.3.0",
    "webpack-cli": "^2.0.13"
  }
}
